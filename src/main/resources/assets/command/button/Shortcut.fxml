<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<Tab text="Shortcut" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1"
     fx:controller="com.getpcpanel.ui.command.button.BtnShortcutController">
    <VBox layoutX="102.0" layoutY="208.0" spacing="15.0">
        <padding>
            <Insets left="8.0" right="8.0" top="8.0"/>
        </padding>
        <Label maxWidth="1.7976931348623157E308" prefHeight="25.0" text="Enter a command or file to launch"/>
        <HBox spacing="10.0">
            <TextField fx:id="shortcutField" prefHeight="27.0" prefWidth="290.0" HBox.hgrow="ALWAYS"/>
            <Button fx:id="scFileButton" mnemonicParsing="false" onAction="#scFile" prefHeight="25.0" prefWidth="34.0" text="..."/>
        </HBox>
    </VBox>
</Tab>
