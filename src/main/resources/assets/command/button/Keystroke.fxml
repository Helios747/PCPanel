<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<HBox xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.getpcpanel.ui.command.button.BtnKeystrokeController">
    <VBox layoutX="181.0" layoutY="200.0" spacing="15.0" HBox.hgrow="ALWAYS">
        <padding>
            <Insets left="8.0" right="8.0" top="8.0"/>
        </padding>
        <Label maxWidth="1.7976931348623157E308" prefHeight="25.0" prefWidth="205.0" text="Enter a keystroke"/>
        <HBox spacing="10.0">
            <TextField fx:id="keystrokeField" editable="false" prefHeight="27.0" prefWidth="278.0" HBox.hgrow="ALWAYS"/>
            <Button mnemonicParsing="false" onAction="#clearKeystroke" text="Clear"/>
        </HBox>
    </VBox>
</HBox>
